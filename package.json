{
  "name": "vscode-color-pick",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.23.0"
  },
  "publisher": "adamwatters",
  "repository": {
    "type": "git",
    "url": "https://github.com/adamwatters/vscode-color-pick"
  },
  "activationEvents": [
    "onCommand:pick-color"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pick-color",
        "title": "Pick Color"
      }
    ]
  },
  "dependencies": {
    "@types/react-color": "^3.0.0",
    "react": "^16.3.2",
    "react-color": "^2.17.3",
    "react-dom": "^16.3.2",
    "react-scripts-ts": "2.16.0"
  },
  "scripts": {
    "vscode:prepublish": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "typescript": "^2.8.3",
    "vscode": "^1.1.17"
  }
}
